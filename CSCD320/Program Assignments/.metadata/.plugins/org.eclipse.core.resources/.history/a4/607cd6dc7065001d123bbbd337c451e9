import java.util.Scanner;

public class TopoSort {

	
	
	public static void main(String[] args) {
		File input = new File(args[0]);
		Scanner scan = null;
		try {
			Scanner scan = new Scanner(input);
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
	}

}
